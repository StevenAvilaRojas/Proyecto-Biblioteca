<p id="notice"><%= notice %></p>

<h1>Myarticles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Article</th>
      <th>Time</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myarticles.each do |myarticle| %>
      <tr>
        <td><%= myarticle.user_id %></td>
        <td><%= myarticle.article_id %></td>
        <td><%= myarticle.time %></td>
        <td><%= myarticle.date %></td>
        <td><%= link_to 'Show', myarticle %></td>
        <td><%= link_to 'Edit', edit_myarticle_path(myarticle) %></td>
        <td><%= link_to 'Destroy', myarticle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myarticle', new_myarticle_path %>
